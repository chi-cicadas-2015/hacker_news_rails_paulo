<% provide(:title, "Log In") %>

<h1>Log in</h1>

<div class="row">
  <div class="col">
    <%= form_for(:session, url: login_path) do |f| %>

      <%= f.label :email %>
      <%= f.email_field :email%>

      <%= f.label :password %>
      <%= f.password_field :password %>

      <%= f.submit "Log in" %>
    <% end %>

    <%= link_to "Sign up", signup_path %>
  </div>
</div>
